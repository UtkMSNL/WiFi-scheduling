<?xml version="1.0"?>
<block>
  <name>Side Channel Append</name>
  <key>ieee802_11_side_channel_mod_cc</key>
  <category>IEEE802.11</category>
  <import>import ieee802_11</import>
  <make>ieee802_11.side_channel_mod_cc($debug, $enable, $mask)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Mask vector</name>
    <key>mask</key>
    <type>float_vector</type>
  </param>

  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>True</value>
    <type>bool</type>
    <option>
    	<name>Enable</name>
    	<key>True</key>
    </option>
    <option>
    	<name>Disable</name>
    	<key>False</key>
    </option>
  </param>

  <param>
    <name>Enable</name>
    <key>enable</key>
    <value>True</value>
    <type>bool</type>
    <option>
    	<name>Yes</name>
    	<key>True</key>
    </option>
    <option>
    	<name>No</name>
    	<key>False</key>
    </option>
  </param> 
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
    <nports>1</nports>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
    <nports>1</nports>
  </source>
</block>
