<?xml version="1.0"?>
<block>
	<name>Chunks to Symbols (VLC)</name>
	<key>ieee802_11_chunks_to_symbols_vlc</key>
	<category>IEEE802.11</category>
	<import>import ieee802_11</import>
	<make>ieee802_11.chunks_to_symbols_vlc($debug)</make>

	<param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>

		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>
	
	<sink>
		<name>in</name>
		<type>byte</type>
		<nports>1</nports>
	</sink>
	<source>
		<name>out</name>
		<type>complex</type>
		<nports>1</nports>
	</source>
</block>
