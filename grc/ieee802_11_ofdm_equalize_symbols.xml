<?xml version="1.0"?>

<block>
	<name>OFDM Equalize Symbols</name>
	<key>ieee802_11_ofdm_equalize_symbols</key>
	<category>IEEE802.11</category>
	<import>import ieee802_11</import>
	<make>ieee802_11.ofdm_equalize_symbols($algo, $debug)</make>
	<callback>set_algorithm($algo)</callback>

	<param>
		<name>Algorithm</name>
		<key>algo</key>
		<value>ieee802_11.LINEAR_COMB</value>
		<type>int</type>

		<option>
			<name>LMS</name>
			<key>ieee802_11.LMS</key>
		</option>
		<option>
			<name>linear comb</name>
			<key>ieee802_11.LINEAR_COMB</key>
		</option>
	</param>

	<param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>

		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>

	<sink>
		<name>in</name>
		<type>complex</type>
                <vlen>64</vlen>
		<nports>1</nports>
	</sink>

	<source>
		<name>out</name>
		<type>complex</type>
		<vlen>48</vlen>
		<nports>1</nports>
	</source>

</block>

